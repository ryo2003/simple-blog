
<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p> <%= startingContent %> </p>


  <%  posts.forEach(function(post){ %>

    <h1><%=post.title%></h1>
    <p>
    <%=post.body.substring(0, 100) + " ..."%>
    <a href="/posts/<%=post.id%>">Read More</a>
    </p>

    <% }) %>

    <button type="button" onclick="window.location.href='/compose';" class="btn btn-primary">New Post</button>


<%- include("partials/footer"); -%>
